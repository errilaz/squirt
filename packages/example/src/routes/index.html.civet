export default ({ request, production }: Context) => list $,
  doctype.html5
  html $,
    head $,
      style $,
        rule "body",
          backgroundColor "black"
          color "silver"
        rule ".hello",
          fontWeight "bold"
    body $,
      div.hello `hello! ${request.url}`, color "lime"
      input { type: "password" }
      liveReload !production
